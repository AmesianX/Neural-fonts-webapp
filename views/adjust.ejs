                </div>
                <div id="range_container2">
                  <p class="ptag">pathomit</p>
                  <input id="pathomit" type="range" min="0" max="2" step="0.1" value="1" style="width:350px;">
                  <p class="ptag">blurradius</p>
                  <input id="blurradius" type="range" min="0" max="2" step="0.1" value="1" style="width:350px;">
                  <p class="ptag">blurdelta</p>
                  <input id="blurdelta" type="range" min="0" max="2" step="0.1" value="1" style="width:350px;">
                  <button class="btn btn-primary" id="save_button">Save</button>
                </div>

              </div>

              </div>
              <div id="main_right">
